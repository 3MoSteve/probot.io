{
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "aqua",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "linebreak-style": 0
  }
}